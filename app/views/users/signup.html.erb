<div class="login-table">
<div class="login-middle">
  <div id="sign-in-up">

    <%= image_tag "vino-bueno-red-2.png" %>

    <form class="" action="<%= users_url %>" method="post">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

      <p>
        <input type="text" name="user[email]" value="" placeholder="Email" class="form-control">
      </p>

      <p>
        <input type="password" name="user[password]" value="" placeholder="Password" class="form-control">
      </p>

      <button type="submit">Sign Up</button>

    </form>

  </div>
</div>
</div>
